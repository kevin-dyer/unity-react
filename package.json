      "smartworks.unity-react/unity-button-react": {
        "env": {
          "compiler": {
            "bit.envs/compilers/react-typescript@3.1.51": {
              "rawConfig": {
                "compilerPath": "typescript/bin/tsc",
                "compilerArguments": [
                  "--declaration"
                ],
                "compiledFileTypes": [
                  ".ts",
                  ".tsx"
                ],
                "configFileName": "tsconfig.json",
                "tsconfig": {
                  "compilerOptions": {
                    "outDir": "dist",
                    "target": "ES2015",
                    "sourceMap": false,
                    "moduleResolution": "Node",
                    "esModuleInterop": true,
                    "module": "ES6",
                    "allowSyntheticDefaultImports": true,
                    "resolveJsonModule": true,
                    "declaration": true,
                    "experimentalDecorators": true,
                    "inlineSourceMap": false,
                    "inlineSources": false,
                    "lib": [
                      "dom",
                      "es2015"
                    ],
                    "jsx": "react",
                    "rootDir": "./",
                    "removeComments": true,
                    "typeRoots": [
                      "./node_modules/@types"
                    ],
                    "importHelpers": false
                  },
                  "include": [
                    "./**/*"
                  ],
                  "exclude": [
                    "node_modules",
                    ".dependencies",
                    "dist"
                  ]
                },
                "development": false,
                "copyPolicy": {
                  "ignorePatterns": [
                    "package.json",
                    "package-lock.json",
                    "tsconfig.json"
                  ],
                  "disable": false
                }
              },
              "options": {},
              "files": {}
            }
          }
        }
      },
